{
      "name": "crhapi",
      "version": "1.0.0",
      "description": "",
      "main": "dist/index.js",
      "scripts": {
            "dev": "ts-node-dev --respawn src/index.ts",
            "build": "tsc",
            "start": "node dist/index.js",
            "bundle": "esbuild src/index.ts --bundle --platform=node --outfile=dist/bundle.js --external:fs --external:path --external:os",
            "obfuscate": "node obfuscate.js",
            "build:sea": "npm run bundle && npm run sea:blob && npm run sea:copy && npm run sea:inject",
            "build:sea-obfuscated": "npm run bundle && npm run obfuscate && npm run sea:blob-obfuscated && npm run sea:copy && npm run sea:inject",
            "sea:blob": "node --experimental-sea-config sea-config.json",
            "sea:blob-obfuscated": "node --experimental-sea-config sea-config-obfuscated.json",
            "sea:copy": "node -e \"require('fs').copyFileSync(process.execPath, 'crhapi-sea.exe')\"",
            "sea:inject": "npx postject crhapi-sea.exe NODE_SEA_BLOB sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2"
      },
      "keywords": [],
      "author": "",
      "license": "ISC",
      "dependencies": {
            "@types/express-session": "^1.18.2",
            "cors": "^2.8.5",
            "dotenv": "^17.2.0",
            "express": "^4.19.2",
            "express-rate-limit": "^7.5.1",
            "express-session": "^1.18.2",
            "helmet": "^8.1.0",
            "http-problem-details": "^0.1.7",
            "mssql": "^11.0.1",
            "nexe": "^4.0.0-rc.6"
      },
      "devDependencies": {
            "@types/cors": "^2.8.19",
            "@types/express": "^4.17.21",
            "@types/mssql": "^9.1.7",
            "esbuild": "^0.25.6",
            "javascript-obfuscator": "^4.1.1",
            "postject": "^1.0.0-alpha.6",
            "ts-node-dev": "^2.0.0",
            "typescript": "^5.5.3"
      }
}
